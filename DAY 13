////////////////////////////////////////////////////////// CODE 1 ////////////////////////////////////////////////////
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n,A,B,X,Y;
	cin>>n;
	for(int i=1;i<=n;i++){
	    cin>>A>>B>>X>>Y;
	    int t=A*B;
	    int s=X*Y;
	    if(t<=s){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}


