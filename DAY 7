/////////////////////////////////////////////// CODE 1 //////////////////////////////////////////////
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int N,X,Y;
	cin>>N;
	for(int i=1;i<=N;i++){
	    cin>>X>>Y;
	    if(X<=10*Y){
	        cout<<"No"<<endl;
	    }
	    else {
	        cout<<"Yes"<<endl;
	    }
	}
	return 0;
}


//////////////////////////////////////////////// CODE 2 /////////////////////////////////////////////////
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,Pa,Pb,Pc;
	cin>>T;
	for(int i=1;i<=T;i++){
	    cin>>Pa>>Pb>>Pc;
	    int X=Pa+Pc;
	    if(X>=Pb){
	        cout<<X<<endl;
	    }
	    else{
	        cout<<Pb<<endl;
	    }
	}    
	return 0;
}
