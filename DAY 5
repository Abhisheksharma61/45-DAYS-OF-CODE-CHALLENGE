//////////////////// CODE 1: ////////////////////
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n,x;
	cin>>n;
	for(int i=1;i<=n;i++){
	    cin>>x;
	    if(x<=15){
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"No"<<endl;
	    }
	    
	}
	return 0;
}


//////////////////////// CODE 2: ///////////////////////
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n,x;
	cin>>x;
	for(int i=1;i<=x;i++){
	    cin>>n;
	    
	        int m=(n/2)+1;
	        cout<<m<<endl;
	    }  
	
	return 0;
}
