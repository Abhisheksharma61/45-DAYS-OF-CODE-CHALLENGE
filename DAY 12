////////////////////////////////////////////////////////// CODE 1 ///////////////////////////////////////
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n,x,y,z;
	cin>>n;
	for(int i=1;i<=n;i++){
	    cin>>x>>y>>z;
	    int t=(y/x);
	    
	    if(z<=t){
	        cout<<"0"<<endl;
	    }
	    else{
	        cout<<z-t<<endl;
	    }
	    
	}
	return 0;
}


///////////////////////////////////////////////////////// CODE 2 /////////////////////////////////////////
#include <iostream>
using namespace std;

int main() {
	int n;
	cin>>n;
	for(int i=1;i<=n;i++){
	    int x,y,l=0,b=0;
	    cin>>x>>y;
	    for(int i=1;;i++){
	       if(i%2==0){
	        b=b+i;
	        if(b>y){
	        cout<<"Limak"<<endl; 
	        break;
	            } 
	        }
	        else{
	        l=l+i;
	        if(l>x) {
	        cout<<"Bob"<<endl;
	        break;
	            }
	        }
	    }
	}
	return 0;
}
