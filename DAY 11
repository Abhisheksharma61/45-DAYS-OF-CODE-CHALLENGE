/////////////////////////////////////////////// CODE 1 //////////////////////////////////////////
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n,x,y;
	cin>>n;
	for(int i=1;i<=n;i++){
	    cin>>x>>y;
	    if((y*6*6)>=x){
	        cout<<"YES"<<endl;
	    }
	    
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
