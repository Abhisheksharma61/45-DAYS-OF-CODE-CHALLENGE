Code 1:
#include <iostream>
using namespace std;

int main() {
	int T,N;
	cin>>T;
	for(int i=1;i<=T;i++)
	{
	    cin>>N;
	  if(N<=4)
	  {
	      cout<<1<<endl;
	  }
	  else
	  {
	      cout<<(N+3)/4<<endl;
	  }
	}
	return 0;
}

Code 2:
#include <iostream>
using namespace std;

int main() {
	int T;
	cin>>T;
	while(T--){
	    int D;      //number of digits in number
	    string N;   //the number N entered as a string
	    cin>>D;
	    cin>>N;
	    int flag = 0;
	   for(int i=0; i<D; i++){
	       if(N[i]=='0'||N[i]=='5'){
	           flag=1;
	           break;
	       }
	   }
	   if(flag==1){
	           cout<<"Yes"<<endl;
	       }
	       else{
	           cout<<"No"<<endl;
	       }
	}
	return 0;
}
